<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape android:shape="rectangle">
			<!-- yellow -->
			<gradient android:startColor="#ADDFFF" android:endColor="#56A5EC"
				android:angle="270" />
		</shape>
	</item>

	<item>
		<shape android:shape="rectangle">
			<!-- gray -->
			<gradient android:startColor="#fff" android:endColor="#eee"
				android:angle="270" />
		</shape>
	</item>
</selector>
